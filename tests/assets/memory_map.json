{
    "protocol": {
        "name": "Example Protocol",
        "addressMax": "0xFFFF_FFFF",
        "addressUnit": 1,
        "addressAlign": 4
    },
    "name": "Example Memory Map",
    "type": "set",
    "contains": {
        "name": "A nice group",
        "address": "0x0000",
        "access": "r",
        "type": "set",
        "contains": [
            {
                "name": "Description String",
                "type": {
                    "string": 20
                },
                "value": "My Great Memory Map"
            },
            {
                "name": "A bitfield",
                "access": "rw",
                "type": {
                    "bitfield": {
                        "length": 16,
                        "bits": {
                            "zero": 0,
                            "one" : 1,
                            "two" : 2,
                            "ten" : 10
                        }
                    }
                }
            },
            {
                "name": "Another bitfield",
                "access": "rw",
                "type": {
                    "bitfield": {
                        "length": 8,
                        "bits": [
                            "item0",
                            "item1",
                            "item2",
                            "item3"
                        ]
                    }
                }
            }
        ]
    }
}
